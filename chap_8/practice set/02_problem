#include <stdio.h>
#include <string.h> // Include this header for strlen

int main() {
    char str[] = "MANISH";
    
    // Loop through each character in the string
    for (int i = 0; i < strlen(str); i++) {
        str[i] = str[i] + 1; // Increment ASCII value of each character by 1
    }
    
    printf("%s", str); // Print the modified string
    return 0;
}
