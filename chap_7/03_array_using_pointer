#include<stdio.h>
int main() {
    int marks[] = {12, 34, 45}; // Initialize array
    int* ptr = marks;           // Pointer to the array

    // Print the elements of the array using the pointer
    for (int i = 0; i < 3; i++) {
        printf("Element %d: %d\n", i, *(ptr + i)); // Access elements using pointer arithmetic
    }

    return 0;
}
